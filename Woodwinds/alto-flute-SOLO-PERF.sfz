//==============================================================
// Alto Flute Solo : Performance
//
//   This is a combination of
//
//     alto-flute-SOLO-sustain.sfz
//     alto-flute-SOLO-staccato.sfz
//
//   ... where high key velocity reduces the sample attack time
//       and the mod wheel increases the volume
//
//   by Paul Battersby - http://virtualplaying.com
//
//==============================================================

<control>
// set some sfz 2.0 default values
// mod wheel
set_cc1=64
// pan (not used but Sforzando will default to 0 = 100%L when
//  other CC settings are present)
set_cc10=64

//==============================================================
// Solo Alto Flute : Sustain
//
//   Alto Flute samples from University of Iowa Electronic Music Studios - http://theremin.music.uiowa.edu/index.html
//
//   modified and mapped by Paul Battersby - http://virtualplaying.com
//
//==============================================================

<group>
// cc1 controls gain after lowering volume of all samples (PB)
gain_cc1=32
// reduce velocity tracking, use cc1 for volume (PB)
amp_veltrack=60

// reduce attack with increased key velocity (PB)
ampeg_attack=1
ampeg_vel2attack=-2

// simulate accent by delaying sustain at higher velocity (PB)
delay_cc1=0.06



// add some pitch vibrato (PB)
pitchlfo_freq=2.531
pitchlfo_depth=10
pitchlfo_delay=1
pitchlfo_fade=1.0

// randomize like a real player(PB)
pitch_random=12
amp_random=1.5
delay_random=0.012




ampeg_release=0.600

volume=-33

<region>
sample=..\libs\Iowa\samples/Alto-Flute-solo/Alto-Flute-sus-A#3-PB-loop.wav
delay_cc1=0.12

